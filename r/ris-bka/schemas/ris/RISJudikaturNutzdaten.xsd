			<xs:element ref="fzinhalt"/>
		</xs:choice>
	</xs:group>
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<xs:element name="kzinhalt">
		<xs:annotation>
			<xs:documentation>Optional: Kopfzeileninhalt für diesen Abschnitt.</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="ueberschrift"/>
				<xs:element ref="absatz"/>
