								<xs:documentation>02_BDGESBLATT</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="repoesterr">
							<xs:annotation>
								<xs:documentation>03_REPOESTERR</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="kurztitel">
							<xs:annotation>
								<xs:documentation>05_Kurztitel</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="entwurf">
							<xs:annotation>
								<xs:documentation>10_Entwurf</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="titel">
							<xs:annotation>
								<xs:documentation>11_Titel</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="g1">
							<xs:annotation>
								<xs:documentation>41_UeberschrG1</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="g1min">
							<xs:annotation>
								<xs:documentation>42_UeberschrG1-</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="g2">
							<xs:annotation>
								<xs:documentation>43_UeberschrG2</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="art">
							<xs:annotation>
								<xs:documentation>44_UeberschrArt+</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="para">
							<xs:annotation>
								<xs:documentation>45_UeberschrPara</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="anlage">
							<xs:annotation>
								<xs:documentation>71_Anlagenbez</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="erlz">
							<xs:annotation>
								<xs:documentation>81_ErlUeberschrZ</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="erll">
							<xs:annotation>
								<xs:documentation>82_ErlUeberschrL</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="tgue">
							<xs:annotation>
								<xs:documentation>89_TGUE_UeberschrSpalte</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="halign"/>
			<xs:attribute name="ct" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<!-- // Preformatted                                          		  -->
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<xs:element name="pre">
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="ueberschrift"/>
				<xs:element ref="absatz"/>
				<xs:element ref="ausgabe"/>
				<xs:element ref="abstand"/>
				<xs:element ref="liste"/>
				<xs:element ref="inhaltsvz"/>
				<xs:element ref="unterschrift"/>
				<xs:element ref="binary"/>
				<xs:element ref="textbox"/>
				<xs:element ref="beschr"/>
				<xs:element ref="table"/>
				<xs:element ref="pre"/>
				<xs:element ref="pdvorlage"/>
				<xs:group ref="zeichenformate" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<!-- // Textboxen                                          -->
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<xs:element name="textbox">
		<xs:complexType mixed="true">
			<xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
