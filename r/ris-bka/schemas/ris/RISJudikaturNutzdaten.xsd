				<xs:annotation>
					<xs:documentation>Verwendete Absatzformatvorlage</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<!-- // Binary -->
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<xs:element name="binary">
		<xs:annotation>
			<xs:documentation>Beliebige Binärdateien werden Base64 eingebettet</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:choice>
					<xs:element ref="src"/>
					<xs:element ref="base64"/>
					<!-- XOR -->
				</xs:choice>
				<xs:element ref="beschr" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="nr" type="xs:integer">
				<xs:annotation>
					<xs:documentation>Laufende Nummer der Grafik</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="datatype" type="xs:string" default="jpeg">
				<xs:annotation>
					<xs:documentation>Gibt den Binärtyp an: jpeg | tiff | pdf | ...</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="shapetype" type="xs:string" default="is">
				<xs:annotation>
					<xs:documentation>Wenn Grafik aus Microsoft Word: is..inline shape, s..shape</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="filename" type="xs:string">
				<xs:annotation>
					<xs:documentation>Ursprünglicher Dateiname der eingebetteten Binärdaten (base64) - keine Pfadangabe.</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="height" type="xs:string">
				<xs:annotation>
					<xs:documentation>Höhe in pt</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="width" type="xs:string">
				<xs:annotation>
					<xs:documentation>Breite in pt</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="brightness" type="xs:string">
				<xs:annotation>
					<xs:documentation>Helligkeit in % (dezimal)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="contrast" type="xs:string">
				<xs:annotation>
					<xs:documentation>Kontrast in % (dezimal)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
			<xs:attribute name="oleclass" type="xs:string"/>
			<xs:attribute name="name" type="xs:string"/>
			<xs:attribute name="alt" type="xs:string"/>
			<xs:attribute name="left" type="xs:string"/>
			<xs:attribute name="top" type="xs:string"/>
			<xs:attribute name="hpos" type="xs:string"/>
			<xs:attribute name="vpos" type="xs:string"/>
			<xs:attribute name="align">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="l">
							<xs:annotation>
								<xs:documentation>linksbündig</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="z">
							<xs:annotation>
								<xs:documentation>zentriert</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="r">
							<xs:annotation>
								<xs:documentation>rechtsbündig</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<!-- ////////////////////////////////////////////////////////////////// -->
	<xs:element name="beschr">
		<xs:annotation>
