			<xs:documentation>BGBl. Ausgabedaten: 04_AusgabeDaten</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:sequence>
				<xs:element ref="adtext"/>
				<xs:element ref="addat"/>
				<xs:element ref="adid"/>
			</xs:sequence>
			<xs:attribute name="ct" type="xs:string" use="optional"/>
