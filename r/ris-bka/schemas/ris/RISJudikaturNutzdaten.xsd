		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="ueberschrift"/>
				<xs:element ref="absatz"/>
				<xs:element ref="ausgabe"/>
				<xs:element ref="abstand"/>
				<xs:element ref="liste"/>
				<xs:element ref="binary"/>
				<xs:element ref="beschr"/>
				<xs:element ref="table"/>
				<xs:element ref="pdvorlage"/>
			</xs:choice>
			<xs:attribute name="typ"/>
			<xs:attribute name="abschnittnr"/>
			<xs:attribute name="ct" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<!-- // Fußnoten / Endnoten                                                       -->
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<xs:element name="fnen">
		<xs:annotation>
			<xs:documentation>Optional Fußnoten/Endnoten</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:choice>
				<xs:element ref="fntext" maxOccurs="unbounded"/>
				<xs:element ref="fnzeichen"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:element name="fntext">
		<xs:complexType mixed="true">
			<xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
			<xs:attribute name="nr" type="xs:nonNegativeInteger"/>
			<xs:attribute name="typ">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="f">
							<xs:annotation>
								<xs:documentation>Fußnote</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="e">
							<xs:annotation>
								<xs:documentation>Endnote</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ct" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="fnzeichen">
		<xs:annotation>
			<xs:documentation>Fußnotenreferenz im Text</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:annotation>
				<xs:documentation>Nummer wird zwischen den Tags ausgegeben</xs:documentation>
			</xs:annotation>
			<xs:sequence>
				<xs:element ref="feld" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="typ">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="f">
							<xs:annotation>
								<xs:documentation>Fußnote</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="e">
							<xs:annotation>
								<xs:documentation>Endnote</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="r">
							<xs:annotation>
								<xs:documentation>Referenz auf Fußnote oder Endnote</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ct" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<!-- // Überschriften                                                             -->
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<xs:element name="ueberschrift">
		<xs:annotation>
			<xs:documentation>Absätze mit Überschriftenformatvorlage formatiert (siehe Attribut "typ").</xs:documentation>
		</xs:annotation>
