	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<xs:element name="adtext">
		<xs:annotation>
			<xs:documentation>Ausgegeben am</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<xs:element name="addat">
		<xs:annotation>
			<xs:documentation>11.01.2003</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<xs:element name="adid">
		<xs:annotation>
			<xs:documentation>11/2003</xs:documentation>
		</xs:annotation>
		<xs:complexType mixed="true">
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<!-- // Kopfzeile / Fußzeile                                        -->
	<!-- //////////////////////////////////////////////////////////////////////////// -->
	<xs:group name="kzfz">
