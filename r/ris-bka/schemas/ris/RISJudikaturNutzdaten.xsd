								<xs:documentation>HeaderFooterPrimary - gilt für alle Seiten</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="f">
							<xs:annotation>
								<xs:documentation>HeaderFooterFirstPage - für die jeweils erste Seite eines Abschnittes</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="e">
							<xs:annotation>
								<xs:documentation>HeaderFooterEvenPages - für jede gerade Seite</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="ct" type="xs:string" use="optional"/>
		</xs:complexType>
